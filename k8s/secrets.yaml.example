# Example secrets file - DO NOT commit actual secrets
# Create actual secrets using kubectl:
# kubectl create secret generic solana-staking-secrets \
#   --from-literal=database-url='postgresql://...' \
#   --from-literal=jwt-secret='your-secret-key' \
#   --from-literal=admin-private-key='base58-encoded-key' \
#   --from-literal=redis-password='your-redis-password'

apiVersion: v1
kind: Secret
metadata:
  name: solana-staking-secrets
type: Opaque
stringData:
  database-url: "postgresql://user:password@host:5432/database"
  jwt-secret: "your-super-secret-jwt-key-change-this"
  admin-private-key: "your-base58-encoded-admin-private-key"
  redis-password: "your-redis-password"

